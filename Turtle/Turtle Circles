#import/load turtle
import turtle

t = turtle.Turtle()
wn = turtle.Screen()
wn.bgcolor("black")
wn.title("turtle fun")

t.speed(10)
t.shape("turtle")
t.pensize(5)

t.color("red")
t.begin_fill()
t.circle(100)
t.end_fill()


t.color("blue")
t.begin_fill()
t.circle(50)
t.end_fill()
    
#waits for a user to close a window
wn.exitonclick()
